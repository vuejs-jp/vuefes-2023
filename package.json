{
  "name": "vuefes-2023",
  "version": "1.0.0",
  "description": "Vue Fes Japan 2023",
  "author": "jiyuujin <jiyuujinunite@gmail.com>",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "test": "vitest",
    "generate:transpile": "nuxt generate",
    "generate:post-transpile": "node scripts/renameDir.js",
    "generate": "npm run generate:transpile && npm run generate:post-transpile",
    "preview": "nuxt preview",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,vue,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .prettierignore",
    "lint": "npm run eslint&& npm run htmllint",
    "lint:fix": "npm run eslint:fix && npm run htmllint:fix",
    "eslint": "eslint . --ext js,jsx,ts,tsx,vue",
    "eslint:fix": "eslint . --ext js,jsx,ts,tsx,vue --fix",
    "htmllint": "markuplint \"./app/**/*.vue\" --config .markuplintrc",
    "htmllint:fix": "markuplint \"./app/**/*.vue\" --config .markuplintrc --format",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@markuplint/vue-parser": "3.2.0",
    "@markuplint/vue-spec": "3.2.0",
    "@nuxt/devtools": "0.2.5",
    "@nuxtjs/device": "3.1.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "cypress": "^12.9.0",
    "eslint": "8.33.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-vuejs-accessibility": "2.1.0",
    "husky": "8.0.3",
    "markuplint": "3.2.0",
    "nuxt": "3.2.3",
    "pinceau": "0.18.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vee-validate": "4.7.4",
    "vite-svg-loader": "4.0.0",
    "vitest": "^0.29.3",
    "vue-gtag": "2.0.2-beta.0",
    "vue-tsc": "1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@markuplint/ml-core",
        "postcss",
        "vite",
        "vue"
      ]
    }
  }
}
